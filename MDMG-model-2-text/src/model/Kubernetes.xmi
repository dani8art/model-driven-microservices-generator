<?xml version="1.0" encoding="UTF-8"?>
<cloud:Kubernetes xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:cloud="http://cloud.kubernetes" xsi:schemaLocation="http://cloud.kubernetes ../metamodel/kubernetes.ecore">
  <services
      deployment="//@deps.0"
      name="MongoDB"
      port="27170"
      targetPort="27170"/>
  <services
      deployment="//@deps.1"
      name="Pets"
      port="80"
      targetPort="80"/>
  <deps replicas="1"
      labels="//@labs.0">
    <containers
        name="MongoDB"
        environments="//@envs.0"/>
  </deps>
  <deps replicas="1"
      labels="//@labs.1">
    <containers
        name="Pets"
        environments="//@envs.0"/>
  </deps>
  <envs name="NODE_ENV"
      value="development"/>
  <labs key="name"
      value="MongoDB"/>
  <labs key="name"
      value="Pets"/>
</cloud:Kubernetes>
    
